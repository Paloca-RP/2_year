<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/TextResult"
        android:layout_width="192dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:rotation="-1"
        tools:layout_editor_absoluteX="144dp"
        tools:layout_editor_absoluteY="296dp" />

    <EditText
        android:id="@+id/TextNum2"
        android:layout_width="197dp"
        android:layout_height="48dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="textPersonName"
        android:rotation="-1"
        tools:ignore="SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="144dp" />

    <Button
        android:id="@+id/btnSoma"
        android:layout_width="48dp"
        android:layout_height="56dp"
        android:rotation="-1"
        android:text="+"
        tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck"
        tools:layout_editor_absoluteX="88dp"
        tools:layout_editor_absoluteY="208dp" />

    <Button
        android:id="@+id/btnSub"
        android:layout_width="40dp"
        android:layout_height="56dp"
        android:rotation="-1"
        android:text="-"
        tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck"
        tools:layout_editor_absoluteX="152dp"
        tools:layout_editor_absoluteY="208dp" />

    <Button
        android:id="@+id/btnMulti"
        android:layout_width="40dp"
        android:layout_height="56dp"
        android:rotation="-1"
        android:text="*"
        tools:layout_editor_absoluteX="212dp"
        tools:layout_editor_absoluteY="208dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:rotation="-1"
        android:text="NÃºmero 2"
        app:layout_constraintEnd_toStartOf="@+id/TextNum2"
        tools:layout_editor_absoluteY="160dp" />

    <EditText
        android:id="@+id/TextNum1"
        android:layout_width="199dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:rotation="-1"
        tools:layout_editor_absoluteX="128dp"
        tools:layout_editor_absoluteY="117dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:rotation="-1"
        android:text="Resultado"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="316dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:rotation="-1"
        android:text="@string/n_mero_1"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toStartOf="@+id/TextNum1"
        app:layout_constraintHorizontal_bias="0.846"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.624" />

    <Button
        android:id="@+id/btnDiv"
        android:layout_width="40dp"
        android:layout_height="56dp"
        android:rotation="-1"
        android:text="/"
        tools:layout_editor_absoluteX="276dp"
        tools:layout_editor_absoluteY="208dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="oaldl"
        android:visibility="visible"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="180dp"
        tools:visibility="visible" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="213dp"
        tools:layout_editor_absoluteY="218dp" />

</androidx.constraintlayout.widget.ConstraintLayout>